package allprograms;

public class Programs {

	public static void main(String [] args) {
		
		System.out.println("-------reverse string without inbuilt functions--------");
		String str = "Automation";
		StringBuilder str2 = new StringBuilder();
		str2.append(str);
		str2=str2.reverse();
		System.out.println(str2);
		System.out.println("---------------------------------------------------------");
	}
	
}
