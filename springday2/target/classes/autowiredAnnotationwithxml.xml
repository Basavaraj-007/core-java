<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->

<context:annotation-config></context:annotation-config>
        
        <bean id="c" class="autowiredAnnotationwithxml.Car"/>
		<bean id="i20" class="autowiredAnnotationwithxml.Engine">
		<property name="model"  value="2018"></property>
		<property name="horsepower" value="2500"></property>
		</bean>        
        
		<bean id="bmw" class="autowiredAnnotationwithxml.Engine">
		<property name="model"  value="2022"></property>
		<property name="horsepower" value="5500"></property>
		</bean>        
        
        
        </beans>